[vpswitch]
s0_switch_to_1_key = <Super>1
s0_switch_to_2_key = <Super>2
s0_switch_to_3_key = <Super>3
s0_switch_to_4_key = <Super>4
s0_switch_to_5_key = <Super>5
s0_switch_to_6_key = <Super>6
s0_switch_to_7_key = <Super>7
s0_switch_to_8_key = <Super>8
s0_switch_to_9_key = <Super>9

[expo]
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[opengl]
s0_texture_filter = 0

[animation]
s0_unminimize_effects = animation:Glide 2;

[resize]
s0_initiate_key = <Super>F8

[scale]
s0_initiate_key = Disabled
s0_initiate_all_key = <Super>w

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9
s0_left_key = <Super>Left
s0_right_key = <Super>Right
s0_up_key = <Super>Up
s0_down_key = <Super>Down
s0_left_window_key = <Shift><Super>Left
s0_right_window_key = <Shift><Super>Right
s0_up_window_key = <Shift><Super>Up
s0_down_window_key = <Shift><Super>Down

[gnomecompat]
s0_command_window_screenshot = gnome-screenshot -w
s0_run_command_terminal_key = <Control><Primary><Alt>t

[core]
s0_active_plugins = core;composite;opengl;decor;grid;place;compiztoolbox;imgpng;vpswitch;mousepoll;snap;regex;resize;wall;unitymtgrabhandles;gnomecompat;session;move;animation;workarounds;fade;scale;expo;ezoom;unityshell;
s0_autoraise = false
s0_autoraise_delay = 500
s0_close_window_key = <Shift><Super>c
s0_raise_window_button = Disabled
s0_lower_window_button = Disabled
s0_minimize_window_key = Disabled
s0_maximize_window_key = Disabled
s0_unmaximize_window_key = Disabled
s0_window_menu_key = <Super>space
s0_show_desktop_key = <Super>d
s0_toggle_window_maximized_key = <Super>m
s0_toggle_window_shaded_key = Disabled

[move]
s0_initiate_key = <Super>F7

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[unityshell]
s0_show_hud = Disabled
s0_launcher_hide_mode = 1
s0_show_launcher = Disabled
s0_keyboard_focus = <Super>a
s0_execute_command = Disabled
s0_panel_first_menu = <Super>p
s0_launcher_switcher_forward = <Alt>Tab
s0_launcher_switcher_prev = <Shift><Alt>Tab
s0_disable_show_desktop = true
s0_alt_tab_forward = <Super>Tab
s0_alt_tab_prev = <Shift><Super>Tab
s0_alt_tab_forward_all = Disabled
s0_alt_tab_prev_all = Disabled
s0_autohide_animation = 2

[place]
s0_mode_matches = name=tmux-main;role=browser;class=Exfalso;class=Smplayer;
s0_mode_modes = 3;3;1;3;
s0_viewport_matches = role=browser;name=tmux-main;
s0_viewport_x_values = 2;1;
s0_viewport_y_values = 1;1;

